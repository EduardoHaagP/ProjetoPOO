   <string notr="true">/* === FUNDO DA TELA E SEÇÕES === */
QDialog[objectName=&quot;TelaListagemVendas&quot;] {
    background-color: #252525; /* Fundo secundário (mais claro) */
}

/* === TEXTOS (LABELS) === */
QLabel {
    color: #F0F0F0;
    background-color: transparent;
}
/* Título principal da tela */
QLabel[objectName=&quot;txt&quot;] {
    font-size: 26pt;
    font-weight: bold;
}

/* === TABELA === */
QTableWidget {
    background-color: #444444;
    color: #F0F0F0;
    gridline-color: #555555;
    alternate-background-color: #3A3A3A;
    border: 1px solid #555555; /* Borda da tabela */
}
QHeaderView::section {
    background-color: #555555;
    color: white;
    padding: 4px;
    border: 1px solid #333333;
}

/* === BOTAO DE ACAO (VERMELHO) === */
QPushButton#botVoltar {
    background-color: #DC3545;
    color: white;
    border-radius: 10px;
    padding: 8px;
    font-weight: bold;
}
QPushButton#botVoltar:hover { background-color: #C82333; }
QPushButton#botVoltar:pressed { background-color: #A71D2A; }


/* === BARRAS DE ROLAGEM (Scrollbar) === */
QScrollBar:vertical {
    border: none;
    background-color: #303030; /* Fundo da barra */
    width: 14px;
    margin: 15px 0 15px 0;
}
QScrollBar::handle:vertical {
    background-color: #666666; /* Cor do &quot;pegador&quot; */
    min-height: 30px;
    border-radius: 7px;
}
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0px; /* Esconde as setas */
}

QScrollBar:horizontal {
    border: none;
    background-color: #303030;
    height: 14px;
    margin: 0 15px 0 15px;
}
QScrollBar::handle:horizontal {
    background-color: #666666;
    min-width: 30px;
    border-radius: 7px;
}
QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    width: 0px;
}</string>
