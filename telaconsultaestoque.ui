/* === TEXTOS (LABELS) === */
QLabel {
    color: #F0F0F0;
    background-color: transparent;
}
/* Tﾃｭtulo principal */
QLabel[objectName=&quot;txt&quot;] {
    font-size: 26pt;
    font-weight: bold;
}

/* === INPUTS &amp; COMBOBOX (Ativos - Visual Muted) === */
QLineEdit, QComboBox {
    background-color: #3A3A3A; /* Fundo cinza muted para o input */
    color: #F0F0F0;
    border: 1px solid #555555;
    border-radius: 5px;
    padding: 5px;
}
QLineEdit:focus, QComboBox:focus {
    border: 1px solid #E67E22; /* Destaque laranja ao focar */
}
QComboBox::drop-down {
    border-left: 1px solid #555555;
    width: 15px;
}

/* 泙 CORREﾃﾃグ CRﾃ控ICA: VISIBILIDADE DO TEXTO NA LISTA DROP-DOWN */
QComboBox QAbstractItemView {
    background-color: #444444; /* Fundo escuro para a lista flutuante */
    color: #F0F0F0;           /* Texto claro e legﾃｭvel */
    selection-background-color: #E67E22; /* Laranja para o item selecionado */
    selection-color: white;             /* Texto branco no item selecionado */
    border: 1px solid #555555;
}

/* === TABELA === */
QTableWidget {
    background-color: #444444;
    color: #F0F0F0;
    gridline-color: #555555;
    alternate-background-color: #3A3A3A;
    border: 1px solid #555555;
}
QHeaderView::section {
    background-color: #555555;
    color: white;
    padding: 4px;
    border: 1px solid #333333;
}

/* === BOTﾃグ DE Aﾃﾃグ PRINCIPAL (Consultar - VERDE) === */
QPushButton#botCadastrar { /* Este ﾃｩ o botﾃ｣o &quot;Consultar&quot; */
    background-color: #28A745;
    color: white;
    border-radius: 10px;
    padding: 8px;
    font-weight: bold;
    border: none;
}
QPushButton#botCadastrar:hover { background-color: #218838; }

/* === BOTﾃグ DE NAVEGAﾃﾃグ (Voltar - VERMELHO) === */
QPushButton#botVoltar {
    background-color: #DC3545;
    color: white;
    border-radius: 10px;
    padding: 8px;
    font-weight: bold;
}
QPushButton#botVoltar:hover { background-color: #C82333; }</string>
