  <property name="styleSheet">
   <string notr="true">/* === FUNDO DAS TELAS (MENOS A INICIAL) === */

QWidget#pagLogin, QWidget#pagPrincipal {

background-color: #1c1b1b;

}



/* === TEXTOS (LABELS) === */

QLabel {

color: #F0F0F0; /* Texto claro (quase branco) */

}



/* === CAIXAS DE INPUT E SELEÇÃO (QLineEdit, QComboBox) === */

QLineEdit, QComboBox {

background-color: #444444; /* Fundo do input */

color: #F0F0F0; /* Texto do input */

border: 1px solid #555555;

border-radius: 5px;

padding: 5px;

}

QLineEdit:focus {

border: 1px solid #E67E22;

}



/* === TABELAS === */

QTableWidget {

background-color: #444444;

color: #F0F0F0;

gridline-color: #555555;

alternate-background-color: #3A3A3A;

}

QHeaderView::section {

background-color: #555555; /* Fundo do cabeçalho */

color: white;

padding: 4px;

border: 1px solid #333333;

}



/* === 1. BOTOES DE ACAO (VERMELHO) === */

QPushButton#botSair, QPushButton#botVoltar {

background-color: #DC3545; /* Vermelho &quot;perigo&quot; */

color: white;

border-radius: 10px; /* Bordas arredondadas */

padding: 8px;

font-weight: bold;

}

QPushButton#botSair:hover, QPushButton#botVoltar:hover {

background-color: #C82333;

}

QPushButton#botSair:pressed, QPushButton#botVoltar:pressed {

background-color: #A71D2A;

}



/* === 2. BOTAO DE ALERTA (AMARELO) === */

QPushButton#botCancelar {

background-color: #FFC107; /* Amarelo &quot;alerta&quot; */

color: #212121; /* Texto escuro */

border-radius: 10px;

padding: 8px;

font-weight: bold;

}

QPushButton#botCancelar:hover {

background-color: #E0A800;

}

QPushButton#botCancelar:pressed {

background-color: #C69500;

}



/* === 3. BOTOES DE NAVEGACAO (TELA PRINCIPAL) === */

QWidget#pagPrincipal QPushButton {

border-radius: 15px;

font-size: 14px;

font-weight: bold;

}



/* Cor 1: Cadastro Cliente (AMARELO NOVO) */

QPushButton#botCadastroCliente {

background-color: #ffc400;

color: #212121; /* Texto escuro (ESSENCIAL para leitura) */

}

QPushButton#botCadastroCliente:hover {

background-color: #E6B000; /* Tom mais escuro para hover */

}

QPushButton#botCadastroCliente:pressed {

background-color: #CC9E00; /* Tom mais escuro para clique */

}



/* Cor 2: Registrar Venda (Laranja Cenoura) */

QPushButton#botVenda {

background-color: #E67E22;

color: #212121;

}

QPushButton#botVenda:hover {

background-color: #D35400;

}

QPushButton#botVenda:pressed {

background-color: #B84A00;

}



/* Cor 3: Listar Vendas (Vermelho Tomate) */

QPushButton#botListaVendas {

background-color: #FF6347;

color: #212121;

}

QPushButton#botListaVendas:hover {

background-color: #E55A40;

}

QPushButton#botListaVendas:pressed {

background-color: #D94F38;

}



/* Cor 4: Consultar Estoque (Laranja Queimado) */

QPushButton#botEstoque {

background-color: #BF360C;

color: #212121;

}

QPushButton#botEstoque:hover {

background-color: #A9310A;

}

QPushButton#botEstoque:pressed {

background-color: #932B09;

}



/* === 4. TELA DE LOGIN === */

QPushButton#botOk {

background-color: #E67E22; /* Laranja (como estava antes) */

color: white;

border-radius: 10px;

padding: 8px;

/* --- REGRA DE FONTE CORRIGIDA --- */

font-family: Segoe UI;

font-size: 12pt;

font-weight: 900;

}

QPushButton#botOk:hover {

background-color: #D35400;

}

QPushButton#botOk:pressed {

background-color: #B84A00;

}



/* Label &quot;Esqueceu sua senha?&quot; (Laranja agora) */

QLabel#txtEsqueceuSenha {

color: #E67E22; /* Laranja (combina com o botão de login) */

text-decoration: underline;

}

QLabel#txtEsqueceuSenha:hover {

color: #FFC107;

}



/* === 5. BOTAO DA TELA INICIAL === */

QPushButton#botEntrar {

background-color: #ffd900;

color: #212121; /* ATUALIZADO: Cor do texto */

border-radius: 30px;

padding: 8px;

/* --- REGRA DE FONTE CORRIGIDA --- */

font-family: Segoe UI;

font-size: 25pt;

font-weight: 900;

}

QPushButton#botEntrar:hover {

background-color: #E6C300;

}

QPushButton#botEntrar:pressed {

background-color: #CCA000;

}



/* === 6. REGRA DA IMAGEM DE FUNDO (MOVIDA PARA CÁ) === */

QWidget#pagInicial {

border-image: url(:/assets/telaInicial.png);

}</string>
